function add_pu(pux,puy,puspr)
	pu = {}
	pu.collide=false
	pu.point=10
	pu.x=pux*8
	pu.y=puy*8
	pu.w=1
	pu.h=1
	pu.spr=puspr
	
	pu.draw = function(this)
		spr(this.spr,this.x,this.y)
	end
	
	add(pickup,pu)
end

function add_ene(enex,eney,enespr)
	ene = {}
	ene.point=15
	ene.x=enex*8
	ene.y=eney*8-8
	ene.d = ed
	ene.w=1
	ene.h=2
	ene.spr=enespr
	
	ene.draw = function(this)
		spr(this.spr,this.x,this.y,this.w,this.h)
	end
	
	ene.mv = function(this)
		if(player.y <= this.y+8)then
				this.y=this.y-0.8
			end
			
			if(player.y >= this.y+8)then
				this.y=this.y+0.8
			end
		
			if(player.x <= this.x)then
				this.x=this.x-0.8
			end
			
			if(player.x >= this.x)then
				this.x=this.x+0.8
			end
 end
	
	add(enemies,ene)
end
